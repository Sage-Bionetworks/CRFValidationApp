{
    "identifier": "HeartRate Measurement",
    "schemaInfo": {
        "identifier": "HeartRate Measurement",
        "revision": 7
    },
    "actions"           : { "cancel": { "iconName": "closeActivity" }},
    "shouldHideActions" : ["goBackward", "skip"],
    "steps":
    [
     {
     "identifier"           : "cameraHeartRate",
     "type"                 : "transform",
